algoritmo "G2-E15"
// Função :
// Autor :
// Data : 28/05/2021
// Seção de Declarações  para de ate passo -1 = regressao
var
num, q : inteiro // quadrado
inicio
escreval (" n q")
num <- 1

repita
 q <- (num * num)
 escreval (num, " ", q)
 num <- num + 1
ate num = 51
fimrepita
fimalgoritmo
